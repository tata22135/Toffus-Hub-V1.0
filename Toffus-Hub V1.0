local MeetingUI = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")

MeetingUI.Name = "MeetingUI"
MeetingUI.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
MeetingUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

ImageLabel.Parent = MeetingUI
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.BackgroundTransparency = 1.000
ImageLabel.Size = UDim2.new(0, 0, 1, 0)
ImageLabel.Image = "rbxassetid://6201774952"

local function TweenScript()
	local script = Instance.new('LocalScript', ImageLabel)

	local Image = script.Parent
	Image:TweenSize(UDim2.fromScale(1, 1), Enum.EasingDirection.In, Enum.EasingStyle.Quart, 0.25, true)
	wait(2.30)
	Image:TweenSize(UDim2.fromScale(1, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quart, 0.3, true)
end
coroutine.wrap(TweenScript)()
